import tkinter as tk

root = tk.Tk()
root.title("Block Menu")

frame = tk.Frame(root)
frame.pack(side=tk.LEFT, fill=tk.Y)

search_label = tk.Label(frame, text="Find Block")
search_label.pack()

search_entry = tk.Entry(frame)
search_entry.pack()

blocks_list = tk.Listbox(frame)
blocks_list.pack(fill=tk.Y)

root.mainloop()
